<!DOCTYPE html>
<head>
    <meta charset="UTF-8">
    <title>Chestionar</title>
</head>
<body>
    <h1>Chestionar</h1>
    <form action="/rezultat-chestionar" method="post">
        <fieldset id="Chestionar">
            <% for(var i = 0; i < intrebari.length; i++) { %>
                <p><%= intrebari[i].intrebare %></p>
                <% for(var j = 0; j < intrebari[i].variante.length; j++) { %>
                    <input type="radio" name="q<%= i %>" value="<%= j %>"/><%= intrebari[i].variante[j] %>
                <% } %>
            <% } %>
            
                </br>
            <input type="submit" value="Submit">
            <p>
            
                    <form action="/logout" method="post">
                        <button type="submit">Deconectare</button>
                    </form>
            
                </p>
        </fieldset>
</body>